package ss.pku.re.domain;

public class Condition {
	private String key;
	private String type;
	private String max;
	private String min;
	
	public Condition(String key,String type,String max,String min){
		this.key = key;
		this.type = type;
		this.max = max;
		this.min = min;
	}
	public String getKey() {
		return key;
	}
	public void setKey(String key) {
		this.key = key;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getMax() {
		return max;
	}
	public void setMax(String max) {
		this.max = max;
	}
	public String getMin() {
		return min;
	}
	public void setMin(String min) {
		this.min = min;
	}
	
	
}
